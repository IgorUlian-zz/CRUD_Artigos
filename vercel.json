{
  "version": 2,
  "framework": null,
  "builds": [
    {
      "src": "/public/index.php",
      "use": "@vercel/php"
    },
    {
      "src": "/api/index.php",
      "use": "@vercel/php"
    }
  ],
  "routes": [
    {
      "src": "/(css|js|images|fonts|favicon.ico)(.*)",
      "dest": "/public/$1$2"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.php"
    }
  ],
  "outputDirectory": "public"
}
